</cell>
<cell alignment="center" valignment="top" topline="true" leftline="true" usebox="none">
\begin_inset Text

\begin_layout Plain Layout
\begin_inset Formula $\begin{matrix}\underset{x}{min}\sum_{i}\frac{1}{a_{i}^{T}x+b_{i}}\\
s.t.\text{\,\,}a_{i}^{T}x+b_{i}>0\\
\,\,\,\,\,c_{i}^{T}x+d_{i}\geq0
\end{matrix}\iff\begin{matrix}\underset{x,t}{\min}\sum t_{i}\\
s.t\left\Vert \begin{matrix}2\\
t_{i}-\left(a_{i}^{T}x+b_{i}\right)
\end{matrix}\right\Vert \leq\left(t_{i}+a_{i}^{T}x+b_{i}\right)\\
c_{i}^{T}x+d_{i}\geq0\forall i
\end{matrix}$
\end_inset


\end_layout

\end_inset
</cell>
<cell alignment="center" valignment="top" topline="true" leftline="true" usebox="none">
