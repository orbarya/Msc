\begin_inset Text

\begin_layout Plain Layout

\lang english
\begin_inset Formula $A\in\mathbb{S}_{n}$
\end_inset

 PD 
\begin_inset Formula $\iff\forall N$
\end_inset

 regular 
\begin_inset Formula $NAN^{\ensuremath{T}}\succ0$
\end_inset


\end_layout

\end_inset
